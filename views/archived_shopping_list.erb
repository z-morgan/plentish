<link rel="stylesheet" href="/stylesheets/archived_shopping_list.css">
<script type='text/javascript' src='/javascripts/archived_shopping_list.js'></script>

<div id='list-pane'>
  <ul>
    <% @items.each do |item| %>
      <li class='list-item'>
        <p><%= item['name'] %> <span class='units'>1 <%= item['units']%></p>
        <a class='' data-name='<%= item['name'] %>' data-units='<%= item['units']%>'>
          <img src='/images/blue_plus.png'>
          My Shopping List
        </a>
      </li>
    <% end %>
  </ul>
